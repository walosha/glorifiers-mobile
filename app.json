{
  "expo": {
    "name": "clinic-app",
    "slug": "clinic-app",
    "description": "An on-line medical app",
    "privacy": "unlisted",
    "sdkVersion": "39.0.0",
    "platforms": ["ios", "android"],
    "version": "1.6.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
